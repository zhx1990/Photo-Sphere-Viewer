{
  "name": "@photo-sphere-viewer/plan-plugin",
  "version": "5.9.0",
  "description": "Photo Sphere Viewer plugin to add a geographic map (via Leaftlet).",
  "homepage": "https://photo-sphere-viewer.js.org/plugins/plan",
  "license": "MIT",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "keywords": [
    "map",
    "leaflet",
    "cartography"
  ],
  "dependencies": {
    "leaflet": "^1.9.4"
  },
  "devDependencies": {
    "@types/leaflet": "^1.9.8"
  },
  "peerDependencies": {
    "@photo-sphere-viewer/core": "5.9.0"
  },
  "scripts": {
    "build": "tsup",
    "watch": "tsup --watch",
    "lint": "tsc --noEmit && eslint . --fix && stylelint \"src/**/*.scss\" --fix",
    "publish-dist": "cd dist && npm publish --tag=$NPM_TAG --access=public",
    "npm-link": "cd dist && npm link"
  },
  "psv": {
    "title": "Photo Sphere Viewer / Plan Plugin",
    "style": true,
    "budget": "30kb"
  }
}
