{
  "name": "@photo-sphere-viewer/stereo-plugin",
  "version": "5.9.0",
  "description": "Photo Sphere Viewer plugin to add a stereo view on mobile devices.",
  "homepage": "https://photo-sphere-viewer.js.org/plugins/stereo",
  "license": "MIT",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "peerDependencies": {
    "@photo-sphere-viewer/core": "5.9.0",
